<div class="container">
  <div class="user-panel">
    <%= form_for @user do |f| %>
      <div class="user-panel-header">
        Edit User
      </div>
      <div class="user-panel-body">
          <div class="row">
            <div class="col s6">
              sss
            </div>
            <div class="col s6">
              <div class="row">
                <div class="col s12">
                  <%= f.label :first_name %>
                  <%= f.text_field :first_name %>
                </div>
              </div>
              <div class="row">
                <div class="col s12">
                  <%= f.label :last_name %>
                  <%= f.text_field :last_name %>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col s12">
              <%= f.label :email %>
              <%= f.text_field :email %>
            </div>
          </div>
          <div class="row">
            <div class="col s12">
              <%= f.label :password %>
              <%= f.text_field :encrypted_password %>
            </div>
          </div>
          <div class="row">
            <div class="col s12">
              <%= f.label :confirmpassword %>
              <%= f.text_field :encrypted_password %>
            </div>
          </div>
      </div>
      <div class="user-panel-footer">
        <a href="#" class="waves-effect waves-light btn"><i class="material-icons left">replay</i>Back</a>
        <%= f.button 'SAVE', class: 'waves-effect waves-light btn' %>
      </div>
    <% end %>
  </div>
</div>
