<div class="form-create_groups">
	<h3>Create new group</h3>
	<%= form_for @group do |f| %>
	<%= f.label :name %>:
	<%= f.text_field :name %><br />
	<%= f.label :description %>:
	<%= f.text_field :description %><br />
	<%= f.collection_check_boxes(:user_ids, User.all, :id, :first_name) %>
	<%= f.submit %>
	<% end %>
</div>