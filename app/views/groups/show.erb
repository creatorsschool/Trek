
<div class="col s4">
	<div class="group-show-container">
	<div class="name-show-group-container">
      Group
    </div>
		<div class="col s12">
			<div class="row">
				<h5>Group name</h5>
				<%= @group.name %>
				<h5>Description</h5>
				<%= @group.description %>
				<h5>Users</h5>
				<% User.all.each do |user| %>
				<%= user.first_name %>
				<%= user.last_name %>
				<% end %>
			</div>	
		</div>
	</div>
</div>
</div>
